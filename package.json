    "@types/jest": "^26.0.23",
    "@types/service-worker-mock": "^2.0.1",
    "@willin/eslint-config": "^0.0.6",
    "eslint": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "jest": "^27.0.1",
    "prettier": "^2.5.1",
    "service-worker-mock": "^2.0.5",
    "ts-jest": "^27.0.1",
    "ts-loader": "^9.2.2",
    "typescript": "^4.5.4",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0"
  },
  "dependencies": {
    "worktop": "^0.7.3"
  }
